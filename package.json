{
  "name": "web-components",
  "version": "0.0.1",
  "private": true,
  "files": ["dist"],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.js",
  "types": "./dist/build-components/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.js"
    }
