    "build-dev": "vite build --watch",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.4.11",
    "axios": "^1.4.0",
